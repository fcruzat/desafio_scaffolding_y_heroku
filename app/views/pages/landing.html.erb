<h1>Pages#landing</h1>
<p>Find me in app/views/pages/landing.html.erb</p>
<table class="table table-hover table-danger">
  <thead>
    <tr>
    
      <th scope="col row">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <br>
  <tbody>
    <tr>
      <% @students.each do |student| %>
      <tr class="table-dark">
        <td><%= student.name %></td>
        <td><%= student.age %></td>
        <td><%= student.email %></td>   
      </tr>
      <% end %>
    </tr>
  </tbody>
</table>
  